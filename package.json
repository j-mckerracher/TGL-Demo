{
  "name": ".",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "npm run build:libs",
    "build:libs": "ng build simulation --configuration development && ng build ui --configuration development && ng build rendering --configuration development && ng build fallback --configuration development && ng build config --configuration development && ng build a11y --configuration development",
    "serve": "ng serve tgl-results-explorer-web",
    "build:prod": "ng build tgl-results-explorer-web --configuration production",
    "lint": "ng lint",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "perf": "ts-node tools/fps-harness/index.ts",
    "spike:rendering": "ts-node --project tools/tsconfig.json tools/rendering-spike/compare.ts",
    "spike:chart": "ts-node --project tools/tsconfig.json tools/chart-spike/compare.ts"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/service-worker": "^20.3.10",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^20.6.0",
    "@angular-eslint/eslint-plugin": "^20.6.0",
    "@angular-eslint/eslint-plugin-template": "^20.6.0",
    "@angular-eslint/template-parser": "^20.6.0",
    "@angular/build": "^20.3.9",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "typescript": "~5.9.2"
  }
