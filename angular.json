{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "tgl-results-explorer-web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/tgl-results-explorer-web",
      "sourceRoot": "projects/tgl-results-explorer-web/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/tgl-results-explorer-web/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/tgl-results-explorer-web/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/tgl-results-explorer-web/public"
              }
            ],
            "styles": [
              "projects/tgl-results-explorer-web/src/styles.scss"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "ngsw-config.json"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "tgl-results-explorer-web:build:production"
            },
            "development": {
              "buildTarget": "tgl-results-explorer-web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/tgl-results-explorer-web/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/tgl-results-explorer-web/public"
              }
            ],
            "styles": [
              "projects/tgl-results-explorer-web/src/styles.scss"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/tgl-results-explorer-web/**/*.ts",
              "projects/tgl-results-explorer-web/**/*.html"
            ]
          }
        }
      }
    },
    "simulation": {
      "projectType": "library",
      "root": "projects/simulation",
      "sourceRoot": "projects/simulation/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/simulation/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/simulation/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/simulation/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "rendering": {
      "projectType": "library",
      "root": "projects/rendering",
      "sourceRoot": "projects/rendering/src",
      "prefix": "lib",
      "architect": {
        "build": {
